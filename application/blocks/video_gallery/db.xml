<?xml version="1.0"?>
<schema version="0.3">
    <table name="btVideoGallery">
        <field name="bID" type="I">
            <key/>
            <unsigned/>
        </field>
        <field name="paddingTop" type="C" size="1">
            <unsigned/>
        </field>
    </table>
    <table name="btVideoGalleryGalleryEntries">
        <field name="id" type="I">
            <key/>
            <unsigned/>
            <autoincrement/>
        </field>
        <field name="bID" type="I">
            <unsigned/>
        </field>
        <field name="sortOrder" type="I"></field>
        <field name="image" type="I">
            <default value="0"/>
            <unsigned/>
        </field>
        <field name="videoLink" type="C" size="255"></field>
    </table>
</schema>